@model IEnumerable<BTL_LWNC_WebAmNhac.Models.Playlist>

@{
    ViewData["Title"] = "Index";
}

<h1>Thư viện</h1>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach(var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}

